{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],	
	"no-underscore-dangle":  ["error", { "allow": ["_team", "_nextIdx"] }],
	"no-plusplus": 0
  }
}